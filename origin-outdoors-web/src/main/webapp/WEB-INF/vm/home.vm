		$(".section ul li .rsp").hide();
		$(".section	 ul li").hover(function(){
			$(this).find(".rsp").stop().fadeTo(500, 0.5);
			$(this).find(".text").stop().animate({left:'0'}, {duration: 500});
		}, function(){
			$(this).find(".rsp").stop().fadeTo(500,0);
			// 滑动消失, 没有这一句则立刻消失
		    $(this).find(".text").stop().animate({left:'318'}, {duration: "fast"});
			$(this).find(".text").animate({left:'-318'}, {duration: 0});
		});
		
		// 图片轮转
	    $(".show_box").turnPic();
	    
	    //切换tab
		$('.bread a').click(function(){
			var i = $(this).index();
			$('.bread a span').removeClass('new').eq(i).addClass('new');
			$('.step').fadeOut(300).eq(i).fadeIn(500);
		});
		
		// 拓展训练图片hover事件方法
		brandPicTurn();
    });
    
    <!-- topNav jquery 1.8.3 -->
	jQuery(".topNav").slide({
		type : "menu",
		titCell : "dd",
		targetCell : "ul",
		delayTime : 0,
		defaultPlay : false,
		returnDefault : true
	});
	
	/* 成功案例  基于jquery1.8.3 */
	$(function(){
        $(".success_case_item").hover(
            function(){
                var that = this;
                sucessItemTimer = setTimeout(function(){
                    $(that).find(".cornerCC").animate({width: 157, height: 203, left: 0, top: 0}, 300, function(){
                        $(that).find("dl").fadeIn(200);
                    });
                }, 100);
            },
            function(){
                var that = this;
                clearTimeout(sucessItemTimer);
                $(that).find("dl").fadeOut(200);
                $(that).find(".cornerCC").stop().animate({width:0, height:0, left:78, top:101}, 300);
            }
        );
        
        // 给一级菜单添加hover事件
        $(".menu-nav").hover(
            function(){
                // 给当前元素添加class=nav-on
                $(this).addClass("nav-on");
                // 让子元素可见
                var subNavClass = ".menu-sub";
                var subNav = $(this).siblings(subNavClass);
                subNav.css("display", "block");
            },
            function(){
                var subNav = $(this).siblings(".menu-sub");
                // 让子元素不可见
                subNav.css("display", "none");
                // 移除class=nav-on
                $(this).removeClass("nav-on");
            }
        );
    });

    /** 
     * 获取二级菜单
     */
	function getNextLevel(num) {
		for ( var i = 0; i <= 6; i++) {
			var navSub = document.getElementById("nav-sub-" + i);
			if (i == num) {
				document.getElementById("mynav" + i).className = "nav-on";
				if (navSub == null || navSub == 'undefined') {
					continue;
				} else {
				    alert("获得匹配: " + num + ", 执行display = block.");
					navSub.style.display = "block";
					alert("执行成功!");
				}
			} else {
				document.getElementById("mynav" + i).className = "";
				if (navSub == null || navSub == 'undefined') {
					continue;
				} else {
					if (navSub.style.display == "block") {
						navSub.style.display = "none";
					}
				}
			}
		}
	}

	function recover(num) {
		for ( var i = 0; i <= 6; i++) {
			if (i == num) {
				var navSub = document.getElementById("nav-sub-" + i);
				if (navSub == null || navSub == 'undefined') {
					;
				} else {
					navSub.style.display = "none";
				}
				document.getElementById("mynav" + i).className = "";
			}
		}
	}
	
	/* 拓展训练展示方法  */
	function brandPicTurn(){
		$(".brand_item").hover(
			function(){
				$(this).children(".brand_name").addClass("brand_change");
			}
			, function(){
				$(this).children(".brand_name").removeClass("brand_change");
			}
		);
	}
	
</script>